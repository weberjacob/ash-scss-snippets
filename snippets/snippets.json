{
  "Text_Transform_Uppsercase": {
    "prefix": "upper",
    "body": [
      "text-transform: uppercase;"
    ],
    "description": "Text transform uppercase"
  },
  "Text_Align_Left": {
    "prefix": "left",
    "body": [
      "text-align: left;"
    ],
    "description": "Text align left"
  },
  "Text_Align_Center": {
    "prefix": "center",
    "body": [
      "text-align: center;"
    ],
    "description": "Text align center"
  },
  "Text_Align_Right": {
    "prefix": "right",
    "body": [
      "text-align: right;"
    ],
    "description": "Text align right"
  },
  "Text_Decoration_Underline": {
    "prefix": "underline",
    "body": [
      "text-decoration: underline;"
    ],
    "description": "Text decoration underline"
  },
  "Font_Style_Italic": {
    "prefix": "italic",
    "body": [
      "font-style: italic;"
    ],
    "description": "Font style italic"
  },
  "Rem_Calc": {
    "prefix": "rem",
    "body": [
      "rem-calc($1);"
    ],
    "description": "Rem calc"
  },
  "Color": {
    "prefix": "color",
    "body": [
      "color(theme-$1);"
    ],
    "description": "Color"
  },
  "Breakpoint_Small": {
    "prefix": "small",
    "body": [
      "@include breakpoint(small) {$1}"
    ],
    "description": "Breakpoint Small"
  },
  "Breakpoint_Small_Only": {
    "prefix": "small only",
    "body": [
      "@include breakpoint(small only) {$1}"
    ],
    "description": "Breakpoint Small Only"
  },
  "Breakpoint_Medium": {
    "prefix": "medium",
    "body": [
      "@include breakpoint(medium) {$1}"
    ],
    "description": "Breakpoint Medium"
  },
  "Breakpoint_Medium_Down": {
    "prefix": "medium down",
    "body": [
      "@include breakpoint(medium down) {$1}"
    ],
    "description": "Breakpoint Medium Down"
  },
  "Breakpoint_Large": {
    "prefix": "large",
    "body": [
      "@include breakpoint(large) {$1}"
    ],
    "description": "Breakpoint Large"
  },
  "Breakpoint_XLarge": {
    "prefix": "xlarge",
    "body": [
      "@include breakpoint(xlarge) {$1}"
    ],
    "description": "Breakpoint XLarge"
  },
  "First_of_Type": {
    "prefix": "first",
    "body": [
      "first-of-type {$1}"
    ],
    "description": "First of type"
  },
  "Last_of_Type": {
    "prefix": "last",
    "body": [
      "last-of-type {$1}"
    ],
    "description": "Last of type"
  },
  "Nth_of_Type": {
    "prefix": "nth",
    "body": [
      ":nth-of-type($2)$0"
    ],
    "description": "Nth of type"
  }
}
